

; MSDISP.INF
; - Installs core display support files, etc.
; - Includes drivers for detected & PnP devices

[version]
CatalogFile=msdisp.cat
LayoutFile= Layout.inf, Layout1.inf, Layout2.inf
signature="$CHICAGO$"
Class=Display
ClassGUID={4d36e968-e325-11ce-bfc1-08002be10318}
Provider=%MS%
SetupClass=BASE

[DestinationDirs]
DefaultDestDir  =11             ;LDID_SYS
DDrawDLL.DelFile=11             ;LDID_SYS
CpqIni.Copy     =10             ;LDID_WIN
VMM32.Copy      =22             ;LDID_VMM32
Compaq.RenFiles =11             ;LDID_SYS

; Base Install Sections
; ---------------------------------------------------------
[BaseWinOptions]
DisplayBase

[DisplayBase]
CopyFiles=System.Copy, VMM32.Copy
UpdateInis=VMM32.Ini
addreg=VMM32.Reg
RenFiles=NCR.RenFiles
DelFiles=DDrawDLL.DelFile
DelReg=DeInstallVGARTD.Reg


[System.Copy]
vga.drv,,,12
vgafull.3gr,,,12
framebuf.drv,,,12
framebuf.dll,,,12

[DDrawDLL.DelFile]      ; Delete obsolete DirectDraw 1.0 driver files
fbdd16.dll
m32dd16.dll
m64dd16.dll
chtdd16.dll
cdd16.dll
cmmdd16.dll
s3dd16.dll
s376dd16.dll
tsngdd16.dll
fbdd32.dll
m32dd32.dll
m64dd32.dll
chtdd32.dll
cdd32.dll
cmmdd32.dll
s3dd32.dll
s376dd32.dll
tsngdd32.dll
s3dd32_765.dll
s3_765.vxd
s3_765.drv

[VMM32.Copy]
vdd.vxd,,,12
vflatd.vxd,,,12

[VMM32.Ini]
wininit.ini, CombineVxDs,,"%22%\vdd.vxd=%11%\vmm32.vxd"
wininit.ini, CombineVxDs,,"%22%\vflatd.vxd=%11%\vmm32.vxd"

[VMM32.Reg]
HKLM,System\CurrentControlSet\Control\VMM32Files,vdd.vxd,1,
HKLM,System\CurrentControlSet\Control\VMM32Files,vflatd.vxd,1,

[SysCfgClasses]
Display,%*PNP0900.DeviceDesc%,ROOT,,%Display.SetupClassName%
                        ;Default to VGA if detection fails or no
                        ;display is detected or if setup /i is run

; The section below has to be empty. The Class classinstaller
; relays on it. Dont add anything to this section.
[EmptySection]

; Install class "Display"
; ---------------------------------------------------------
[ClassInstall]
AddReg=ClassAddReg
DelReg=ClassDelReg

[ClassAddReg]
HKR,,,,%ClassName%
HKR,,Installer,,"SetupX.Dll, Display_ClassInstaller"
HKR,,Icon,,"-1"
HKR,,EnumDriverStack,,"enumfile.dll,EnumDisplayDriverStack"

[ClassDelReg]
HKLM,System\CurrentControlSet\Services\Class\Display,EnumPropPages

; Device IDs to exclude from Setup pick list
; ---------------------------------------------------------
[ControlFlags]
ExcludeFromSelect=*PNP0917                        ; Special ID
ExcludeFromSelect=*ATI4402                        ; ATI EISA mach32
ExcludeFromSelect=PCI\VEN_1005&DEV_2301           ; Avance Logic PCI
ExcludeFromSelect=PCI\VEN_100E&DEV_9001           ; Weitek P9000
ExcludeFromSelect=PCI\VEN_100E&DEV_9100           ; Weitek P9100
ExcludeFromSelect=PCI\VEN_EDD8&DEV_A091           ; ARK Logic
ExcludeFromSelect=PCI\CC_0300                     ; Standard PCI Graphics Adapter (VGA)
ExcludeFromSelect=PCI\CC_0301                     ; Standard PCI Graphics Adapter (XGA)
ExcludeFromSelect=PCI\CC_0380                 	  ; Standard PCI Graphics Adapter
ExcludeFromSelect=NullId1,NullId2,NullId3,NullId4 ; Unsupported PCI adapters

