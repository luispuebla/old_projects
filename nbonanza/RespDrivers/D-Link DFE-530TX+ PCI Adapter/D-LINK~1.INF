CatalogFile    = NETDLKR5.CAT                               ;; for WHQL certified
CatalogFile.NT = NETDLKRN.CAT
DriverVer      = 08/23/2001,5.397.0823.2001

[Manufacturer]
%D-Link%   = D-Link

[ControlFlags]
;;ExcludeFromSelect     = PCI\VEN_1186&DEV_1300
;;ExcludeFromSelect     = PCI\VEN_1186&DEV_1300&SUBSYS_13011186                 
;;ExcludeFromSelect     = PCI\VEN_10EC&DEV_8139

[D-Link]
%DFE530.DeviceDesc%  = DFE530.ndi, PCI\VEN_1186&DEV_1300
%DFE530_A.DeviceDesc%  = DFE530_A.ndi, PCI\VEN_1186&DEV_1300&SUBSYS_13011186
%DFE530_A.DeviceDesc%  = DFE530_A.ndi, PCI\VEN_10EC&DEV_8139&SUBSYS_13011186

;;****************************************************************************
;; Windows 9x
;;****************************************************************************
[DFE530.ndi]
DriverVer       = 08/23/2001,397
AddReg          = DFE530.id.reg, DFE530.common.reg, DFE530.win.reg, DFE530.oncbs.reg
CopyFiles       = DFE530.CopyFiles98
;DeviceID        = "PCI\VEN_1186&DEV_1300"
;LogConfig       = DefaultConfig

[DFE530_A.ndi]
DriverVer       = 08/23/2001,397
AddReg          = DFE530.id.reg, DFE530.common.reg, DFE530.win.reg, DFE530.oncbs.reg
CopyFiles       = DFE530.CopyFiles98
;DeviceID        = "PCI\VEN_1186&DEV_1300&SUBSYS_13011186"
;LogConfig       = DefaultConfig

;;****************************************************************************
;; Win2K, WinXP
;;****************************************************************************
[DFE530.ndi.NT]
AddReg          = DFE530.common.reg, DFE530.reg, DFE530.oncbs.reg
Characteristics = 0x84
BusType         = 5
CopyFiles       = DFE530.CopyFiles

[DFE530.ndi.NT.Services]
AddService = DLKRTS, 2, rtsnt.Service, DFE530.EventLog

[DFE530_A.ndi.NT]
AddReg          = DFE530.common.reg, DFE530.reg, DFE530.oncbs.reg
Characteristics = 0x84
BusType         = 5
CopyFiles       = DFE530.CopyFiles

[DFE530_A.ndi.NT.Services]
AddService = DLKRTS, 2, rtsnt.Service, DFE530.EventLog

[DFE530.CopyFiles]
DLKRTS.SYS,,,2

[DFE530.CopyFiles98]
DLKRTS.SYS,,,2

;;----------------------------------------------------------------------------
;; DFE-530TX+ Parameters
;;----------------------------------------------------------------------------
;;***************************************************************************
;; Windows 9x parameters
;;***************************************************************************
[DFE530.id.reg]
HKR, Ndi,       DeviceID,       0,      "PCI\VEN_1186&DEV_1300"

HKR, Ndi\Install,                       ,       0,      rtsnt.install
HKR, NDI\Params\DuplexMode,             flag,   1,      30, 00, 00, 00
HKR, Ndi\Params\EarlyTxThreshold,       flag,   1,      20, 00, 00, 00
HKR, NDI\Params\NetworkAddress,         flag,   1,      30, 00, 00, 00
HKR, NDI\Params\WakeUpFrame,            flag,   1,      30, 00, 00, 00

[DFE530.win.reg]
;; key,subkey,valuname,type,value
HKR, ,  DriverDesc,     0,      "D-Link DFE-530TX+ PCI Adapter"
HKR, ,  DevLoader,      0,      *ndis
HKR, ,  DeviceVxDs,     0,      DLKRTS.SYS
HKR, ,  EnumPropPages,  0,      netdi.dll,      EnumPropPages

;;params
HKR, ,  MediaType,      0,      1
HKR, ,  AdapterType,    0,      5
HKR, ,  BusType,        0,      5

;; NDIS Info
HKR, NDIS,              MajorNdisVersion,       1,      03
HKR, NDIS,              MinorNdisVersion,       1,      0a
HKR, NDIS,              LogDriverName,          0,      "DLKRTS"

;; Interfaces
HKR, Ndi\Interfaces,    DefLower,               ,       "ethernet"
HKR, Ndi\Interfaces,    LowerRange,             ,       "ethernet"
HKR, Ndi\Interfaces,    DefUpper,               ,       "ndis3"
HKR, Ndi\Interfaces,    UpperRange,             ,       "ndis3"

;; Install sections
HKR, Ndi\Install,       ndis3,                  0,      "DFE530.ndis5"

